EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Nimble.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Ono.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Quick.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/cmark.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Nimble" -framework "Ono" -framework "Quick" -framework "cmark"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-CocoaMarkdownTests-iOS
PODS_ROOT = ${SRCROOT}/Pods